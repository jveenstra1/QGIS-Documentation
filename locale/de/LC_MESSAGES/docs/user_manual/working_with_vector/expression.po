msgstr ""
"Liefert die Geometrie des aktuellen Objekts (kann zur Verarbeitung mit "
"anderen Funktionen verwendet werden)"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "$length"
msgstr "$length"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "Returns the length of the current line feature"
msgstr "Liefert die Länge des aktuellen Objekts"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "$perimeter"
msgstr "$perimeter"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "Returns the perimeter of the current polygon feature"
msgstr "Liefert die Umfanglänge des aktuellen Objekts."

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "$x"
msgstr "$x"

