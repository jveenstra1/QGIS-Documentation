msgid "Returns the X coordinate of the current feature"
msgstr "Liefert die X-Koordinate des aktuellen Objektes"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "$x_at(n)"
msgstr "$x_at(n)"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid ""
"Returns the X coordinate of the nth node of the current feature's geometry"
msgstr ""
"Liefert die X-Koordinate des n-ten Knotens der aktuellen Objektgeometrie"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "$y"
msgstr "$y"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "Returns the Y coordinate of the current feature"
msgstr "Liefert die Y-Koordinate des aktuellen Objektes"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "$y_at(n)"
msgstr "$y_at(n)"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid ""
"Returns the Y coordinate of the nth node of the current feature's geometry"
msgstr ""
"Liefert die Y-Koordinate des n-ten Knotens der aktuellen Objektgeometrie"

